{
  "name": "@volt.camp/volt.camp",
  "version": "0.0.0-development",
  "description": "Source code for volt.camp, using TypeScript, Gatsby, and Contentful",
  "author": "Markus Tacker",
  "keywords": [
    "gatsby",
    "typescript",
    "contentful"
  ],
  "scripts": {
    "develop": "NODE_OPTIONS=--no-experimental-fetch gatsby develop",
    "start": "NODE_OPTIONS=--no-experimental-fetch gatsby develop",
    "build": "NODE_OPTIONS=--no-experimental-fetch gatsby build",
    "serve": "NODE_OPTIONS=--no-experimental-fetch gatsby serve",
    "clean": "NODE_OPTIONS=--no-experimental-fetch gatsby clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.181",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-helmet": "6.1.6",
    "eslint": "8.34.0",
    "gatsby": "4.25.4",
    "gatsby-plugin-dts-css-modules": "3.0.0",
    "gatsby-plugin-image": "2.25.0",
    "gatsby-plugin-manifest": "4.25.0",
    "gatsby-plugin-mdx": "3.20.0",
    "gatsby-plugin-react-helmet": "5.25.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sharp": "4.25.0",
    "gatsby-plugin-sitemap": "5.25.0",
    "gatsby-source-contentful": "7.23.1",
    "gatsby-source-filesystem": "4.25.0",
    "gatsby-transformer-sharp": "4.25.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
